# Discord Bot Token
# Get it from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Discord Channel ID to monitor (your "schedule" channel)
# Right-click the channel > Copy ID (enable Developer Mode in Discord settings)
DISCORD_SCHEDULE_CHANNEL_ID=your_channel_id_here

# GitHub Personal Access Token
# Create one at https://github.com/settings/tokens with 'repo' scope
GITHUB_TOKEN=your_github_token_here

# GitHub Repository Info
GITHUB_OWNER=999none
GITHUB_REPO=mk8dx-hub
GITHUB_BRANCH=emergent
GITHUB_FILE_PATH=data/sq-schedule.json
